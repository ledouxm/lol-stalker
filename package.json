{
    "name": "vite-reactts-electron-starter",
    "version": "0.5.0",
    "license": "MIT",
    "main": "main/index.js",
    "author": {
        "name": "Maximilian St√ºmpfl"
    },
    "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
    "keywords": [
        "vite",
        "react",
        "electron",
        "typescript"
    ],
    "repository": {
        "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
    },
    "productName": "vite-reactts-electron-starter",
    "scripts": {
        "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
        "dev:vite": "vite",
        "dev:electron": "npm run build:electron && electron .",
        "build": "npm run build:vite && npm run build:electron",
        "build:vite": "vite build",
        "build:electron": "tsc -p electron",
        "dist": "npm run build && electron-builder",
        "pack": "npm run build && electron-builder --dir",
        "clean": "rimraf dist main src/out",
        "type-check": "tsc"
    },
    "dependencies": {
        "@chakra-ui/react": "^1.6.7",
        "@emotion/react": "^11",
        "@emotion/styled": "^11",
        "@pastable/core": "^0.1.14",
        "electron-is-dev": "^2.0.0",
        "framer-motion": "^4",
        "jotai": "^1.3.5",
        "leaflet": "^1.7.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-leaflet": "^3.2.1"
    },
    "devDependencies": {
        "@types/leaflet": "^1.7.5",
        "@types/node": "^16.3.3",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@vitejs/plugin-react-refresh": "^1.3.1",
        "autoprefixer": "^10.3.1",
        "concurrently": "^6.0.0",
        "electron": "^13.1.7",
        "electron-builder": "^22.10.5",
        "postcss": "^8.3.5",
        "typescript": "^4.2.3",
        "vite": "^2.1.2"
    },
    "build": {
        "asar": true,
        "files": [
            "main",
            "src/out"
        ],
        "directories": {
            "buildResources": "resources"
        }
    }
}
